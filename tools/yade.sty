\newcommand{\cs}[2][draw,->]{ %
  \path[#1] (#2-1-1) -- (#2-1-2) ; %
  \path[#1] (#2-1-3) -- (#2-1-2) ; %
}

\newcommand{\cospan}{\cs{m}}

\newcommand{\vdoublecs}[2][draw,->]{
    \foreach \i in {1,2,3} %
    {%
    \path[#1] (#2-1-\i)  -- (#2-2-\i) ;%
    \path[#1] (#2-3-\i) -- (#2-2-\i) ; %
  }    ; %
}

\newcommand{\hdoublecs}[2][draw,->]{
